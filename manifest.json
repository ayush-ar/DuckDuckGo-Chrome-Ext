{
  "description": "Adds a button to open current search with DuckDuckGo",
  "manifest_version": 2,
  "name": "DuckDuckGo",
  "version": "1.1",
  "homepage_url": "https://github.com/ra101/DuckDuckGo-Chrome-Ext",
  "icons": {
    "256": "icon256.png"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon256.png"
  },
  "commands": {
    "ddg": {
      "suggested_key": {
        "default": "Alt+Shift+D"
      },
      "description": "Open with DuckDuckGo"
    }
  }
}